{
    "name": "ivory/google-map",
    "description": "Google Map API v3 integration for PHP ^8.1",
    "keywords": [ "google", "map"],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Breu",
            "email": "samuel.breu@bresam.ch",
            "role": "Developer/Maintainer"
        },
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com",
            "role": "Creator/Inactive"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/event-dispatcher": "^6.4 || ^7.0",
        "symfony/serializer": "^6.4 || ^7.0",
        "symfony/property-access": "^6.4 || ^7.0",
        "phpdocumentor/reflection-docblock": "^5.3",
        "php-http/message-factory": "^1.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.49",
        "php-http/cache-plugin": "^2.0",
        "php-http/guzzle7-adapter": "^1.0",
        "phpunit/phpunit": "^12.3",
        "symfony/cache": "^6.4 || ^7.0",
        "symfony/phpunit-bridge": "^6.4 || ^7.0",
        "ext-json": "*",
        "symfony/panther": "^2.2",
        "dbrekelmans/bdi": "^1.4"
    },
    "scripts": {
        "test": "vendor/bin/phpunit -c phpunit.ci.xml"
    },
    "suggest": {
        "php-http/client-implementation": "Allows to use http services",
        "php-http/message": "Allows to use http services"
    },
    "replace": {
        "egeloen/google-map": "^2.0.2"
    },
    "autoload": {
        "psr-4": { "Ivory\\GoogleMap\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Ivory\\Tests\\GoogleMap\\": "tests/" }
    }
}
